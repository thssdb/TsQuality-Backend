<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--namespace指向mapper.xml所对应的Mapper接口-->
<mapper namespace="cn.edu.tsinghua.tsquality.mapper.IoTDBSeriesMapper">
    <update id="createSeriesTable" parameterType="String">
        CREATE TABLE IF NOT EXISTS ${tableName}(
            sid INTEGER PRIMARY KEY AUTO_INCREMENT,
            ts_path VARCHAR(255) NOT NULL UNIQUE
        )
    </update>

</mapper>
